    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-table": "^7.7.20",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.11.0",
    "@vitejs/plugin-react": "^4.2.1",
